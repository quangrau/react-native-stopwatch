"env": {
  "browser": true,
    "node": true,
    "es6": true
},
  "settings": {
    "ecmascript": 6
  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true,
    "blockBindings": true,
    "module": true,
    "jsx": true,
  },
  "rules": {
    "react/jsx-boolean-value": 0,
    "new-cap": 0,
    "id-length": 0,
    "no-empty-label": 0,
    "no-multi-spaces": 0,
    "key-spacing": 0,
    "space-before-keywords": 0,
    "space-after-keywords": 0,
    "space-return-throw-case": 0,
    "no-underscore-dangle": ["error", { "allowAfterThis": true }]
  }
}

